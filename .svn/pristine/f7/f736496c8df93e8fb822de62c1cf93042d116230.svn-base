$(function(){
	$.ajax({
		url : "/mem/getTimeData",
		type : "POST",
		success : function(result){
			var r = result.model.timeData;
			chart1(r);
			chart2(r);
		},
		error : function(e){
			alert("erro : r"+e);
		}
	});

	$(".tabmenu>li").eq(2).addClass("on");
});

function chart1(r){
	var title = {
            text: '시간대별 수치통계'
         };
         var subtitle = {
            text: ''
         };
         var xAxis = {
            categories: ['SO2','CO2','O3','NO2']
         };
         var yAxis = {
            title: {
               text: '농도(mg)'
            },
            plotLines: [{
               value: 0,
               width: 1,
               color: '#808080'
            }]
         };
         var tooltip = {
            valueSuffix: 'mg'
         }
         var legend = {
            layout: 'vertical',
            align: 'right',
            verticalAlign: 'middle',
            borderWidth: 0
         };
         var series = r;
                        /*{name: '12:00', data: [7.0, 6.9, 9.5, 14.5, 18.2, 21.5]},
                        {name: '13:00', data: [-0.2, 0.8, 5.7, 11.3, 17.0, 22.0]},
                        {name: '14:00', data: [3.9, 4.2, 5.7, 8.5, 11.9, 15.2]}*/
         var json = {};
         json.title = title;
         json.subtitle = subtitle;
         json.xAxis = xAxis;
         json.yAxis = yAxis;
         json.tooltip = tooltip;
         json.legend = legend;
         json.series = series;
          $('#container1').highcharts(json);
}

function chart2(r){
    var chart = {
       type: 'column'
    };
    var title = {
       text: '시간대별 수치통계'
    };
    var subtitle = {
       text: ''
    };
    var xAxis = {
       categories: ['SO2','CO2','O3','NO2'],
       crosshair: true
    };
    var yAxis = {
       min: 0,
       title: {
          text: '농도(mg)'
       }
    };
    var tooltip = {
       headerFormat: '<span style = "font-size:10px">{point.key}</span><table>',
       pointFormat: '<td style = "padding:0"><b>{point.y}mg</b></td></tr>',
       footerFormat: '</table>',
       shared: true,
       useHTML: true
    };
    var plotOptions = {
       column: {
          pointPadding: 0.2,
          borderWidth: 0
       }
    };
    var credits = {
       enabled: false
    };
    var series = r;
    var json = {};
    json.chart = chart;
    json.title = title;
    json.subtitle = subtitle;
    json.tooltip = tooltip;
    json.xAxis = xAxis;
    json.yAxis = yAxis;
    json.series = series;
    json.plotOptions = plotOptions;
    json.credits = credits;
    $('#container2').highcharts(json);
}
