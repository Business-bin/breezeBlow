<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>

	<typeAliases>
		<typeAlias type="com.brb.brbcom.common.collections.BrbMap" alias="brbMap" />
	</typeAliases>

	<mappers>
		<mapper resource="cust/sqlmap/sql/sample/sampleSql.xml" />
		<mapper resource="cust/sqlmap/sql/login/loginSql.xml" />
		<mapper resource="cust/sqlmap/sql/common/commonSql.xml" />
		<mapper resource="cust/sqlmap/sql/mem/memSql.xml" />
		<mapper resource="cust/sqlmap/sql/product/productSql.xml" />
		<mapper resource="cust/sqlmap/sql/admin/adminSql.xml" />
		<mapper resource="cust/sqlmap/sql/btb/btbSql.xml" />
		<mapper resource="cust/sqlmap/sql/board/boardSql.xml" />
		<mapper resource="cust/sqlmap/sql/sms/smsSql.xml" />
		<mapper resource="cust/sqlmap/sql/app/polySql.xml" />
		<mapper resource="cust/sqlmap/sql/app/appSql.xml" />
		<mapper resource="cust/sqlmap/sql/main/mainSql.xml" />
	</mappers>
</configuration>