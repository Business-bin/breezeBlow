function logout(){
	$.ajax(
			{async : true
			,type : "POST"
			,url : "/login/logout"
			,dataType : "json"
			,data: {R_SADM_EMAIL : $('#loginId').val(),
				    R_SADM_PWD : $('#loginPw').val()
			 }
			,success : function(data) {
				location.href="/";
			}
			,error:	function(request,status,error){

			}
	});
}