.container_body .navi ul li:after {content:">"; padding:0 5px;}
.container_body .navi ul li:last-child:after {content:""; padding-right:0px;}

.login {
	color:#FFF;
	text-align:center;
	background: #000;  /* fallback for old browsers */
	background: -webkit-linear-gradient(to bottom, #000046, #000);  /* Chrome 10-25, Safari 5.1-6 */
	background: linear-gradient(to bottom, #000046, #000); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
}
.login .loginform {position:absolute; top:50%; left:50%; margin-top:-102.5px; margin-left:-148px;}
.login .loginform li.title {text-align:center; font-size:20px; color:#FFF;}
.login .loginform li {text-align:center; padding:5px 0;}
.login .loginform li input[type="text"] {width:280px; border-radius:0px;}
.login .loginform li input[type="password"] {width:280px; border-radius:0px;}
.login .loginform li input[type="submit"] {width:294px; border-radius:0px;}
.login .loginform li input[type="button"] {width:294px; border-radius:0px;}
.login .loginform li button {width:144px;}
.login .loginmessage {text-align:center; font-size:30px; position:absolute; top:50%; width:100%; margin-top:-6px;}