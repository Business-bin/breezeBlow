@charset "utf-8";
@import url("http://fonts.googleapis.com/earlyaccess/nanumgothic.css");

/*===========================Reset============================*/

html, body{height:100%; margin:0px; padding:0px;}
/* reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,textarea,p,blockquote,th,td,input,select,textarea,button {margin:0; padding:0;}
fieldset,img{border:0 none;}
dl,ul,ol,menu,li {list-style:none;}
blockquote, q {quotes:none;}
blockquote:before, blockquote:after,q:before, q:after {content:''; content:none;}
img,input,select,textarea,button,i {vertical-align:middle;}
hr, input::-ms-clear {display:none;}
button {border:0 none; background-color:transparent; cursor:pointer;}
body,th,td,input,select,textarea,button {font-size:12px; line-height:1.5; font-family:'Nanum Gothic', 'Nanum Barun Gothic', '돋움', dotum, sans-serif; color:#666;} /* color값은 디자인가이드에 맞게사용 */
.os_mac body,.os_mac th,.os_mac td,.os_mac input,.os_mac select,.os_mac textarea,.os_mac button {font-family:apple SD gothic neo,'돋움',dotum,sans-serif;}
a {color:inherit; text-decoration:none; outline:none;}
/*a:active, a:hover {text-decoration:underline;}*/
a:active {background-color:transparent;}
address,caption,cite,code,dfn,em,var {font-style:normal; font-weight:normal; ;}
html, body {height:100%; margin:0px; padding:0px;}
table,th,td {border-collapse:collapse; padding:0; border-spacing:0;}
table {width:100%; margin:1px 0;}

/*===========================Input Reset============================*/

input[type="button"],
input[type="submit"],
input[type="reset"] {height:29px; line-height:20px; border:1px solid #2c3447; background:#3e455b; color:#FFF; border-radius:3px; padding:0 5px; cursor:pointer;}

input[type="button"].searchBtn,
input[type="submit"].searchBtn,
input[type="reset"].searchBtn {height:35px; line-height:35px; min-width:100px; border:1px solid #2c3447; background:#3e455b; color:#FFF; border-radius:3px; padding:0 5px; font-size:16px; cursor:pointer;}

input[type="button"].Bicbutton,
input[type="reset"].Bicbutton,
input[type="submit"].Bicbutton {min-width:80px; height:30px; font-size:16px;}

input[type="text"],
input[type="password"],
input[type="number"] {height:27px; border-top:1px solid #cacaca; border-left:1px solid #cacaca; border-bottom:1px solid #e4e4e4; border-right:1px solid #e4e4e4; background:#ffffff; font-size:12px; padding:0 5px; letter-spacing:0px; margin:2px;}
input[type="text"]:disabled{background-color: #e3e3e3;}
input[type="text"]:readonly{background-color: #e3e3e3;}
input[type="file"] {height:30px; margin:2px;}

textarea{width:98%; border-top:1px solid #cacaca; border-left:1px solid #cacaca; border-bottom:1px solid #e4e4e4; border-right:1px solid #e4e4e4; background:#ffffff; font-size:12px; padding:5px; letter-spacing:0px; margin:2px;}
select {vertical-align:middle; margin:2px; height:29px; line-height:29px; border-top:1px solid #cacaca; border-left:1px solid #cacaca; border-bottom:1px solid #e4e4e4; border-right:1px solid #e4e4e4; background:#ffffff; font-size:12px;}
select:disabled{background-color: #e3e3e3;}
select:readonly{background-color: #e3e3e3;}
.clear {clear:both;}

.radios {vertical-align:middle; margin:0 5px;}
.radios input[type="radio"]{display:none; vertical-align:middle;}
.radios input[type="radio"] + label{display:inline-block; background:url('/images/icon/radio_off.png') left center no-repeat; cursor:pointer; padding-left:25px; margin:0px;}
.radios input[type="radio"]:checked + label {background:url('/images/icon/radio_on.png') left center no-repeat; font-weight:600; color:#000; margin:0px;}

.checkboxs {vertical-align:middle; margin:0 5px;}
.checkboxs input[type="checkbox"]{display:none;}
.checkboxs input[type="checkbox"] + label{display:inline-block; background:url('/images/icon/checkbox_off.png') left center no-repeat; cursor:pointer; padding-left:25px; margin:0px;}
.checkboxs input[type="checkbox"]:checked + label {background:url('/images/icon/checkbox_on.png') left center no-repeat; margin:0px;}

.button { padding:5px 10px; display:inline-block; border:1px solid #757a88; font-size:12px; text-align:center; color:#fff; background:#757a88; cursor:pointer;}
.button i,
.button2 i {font-size:18px; vertical-align:middle;}
.button2 {vertical-align:middle; padding:5px 10px; display:inline-block; border:1px solid #757a88; font-size:12px; text-align:center; color:#757a88; font-weight:600;}
.button:hover,
.button2:hover {color:orange;}

.more {min-width:55px; height:20px; line-height:18px; font-size:9px; border:1px solid #a38d68; background:#a38d68; text-align:center; color:#fff;}

.fred {color:#FF0000;}
.button_area {margin:20px 0; text-align:right;}
.tleft {text-align:left;}
.tcenter {text-align:center;}
.tright {text-align:right;}

.basictables {width:100%;}
.basictables th { padding:5px; border:1px solid #d1d1d1; background:#757a88; color:#FFF;}
.basictables td { padding:5px; border:1px solid #d1d1d1; }
.basictables td.tleft {text-align:left;}
.basictables td.tcenter {text-align:center;}
.basictables td.tright {text-align:right;}

.basichover tr:hover {background:#eee;}
.basichover tr:hover td {font-weight:600;}

.buyertables {width:100%;}
.buyertables th { padding:5px; border-bottom:1px solid #d1d1d1;}
.buyertables td { padding:5px; border-bottom:1px solid #d1d1d1; }
.buyertables td.tleft {text-align:left;}
.buyertables td.tcenter {text-align:center;}
.buyertables td.tright {text-align:right;}

.boardtables {width:100%;}
.boardtables th { padding:10px 5px; border-bottom:1px solid #d1d1d1; border-top:1px solid #d1d1d1; background:#eee;}
.boardtables td { padding:10px 5px; border-bottom:1px solid #d1d1d1; }
.boardtables td.tleft {text-align:left;}
.boardtables td.tcenter {text-align:center;}
.boardtables td.tright {text-align:right;}

.boardtablesview {width:100%; border-top:2px solid orange;}
.boardtablesview th { padding:10px 5px; border-bottom:1px solid #d1d1d1; border-top:1px solid #d1d1d1; background:#eee; font-size:14px;}
.boardtablesview td { padding:10px 5px; border-bottom:1px solid #d1d1d1; font-size:14px; }
.boardtablesview td.tleft {text-align:left;}
.boardtablesview td.tcenter {text-align:center;}
.boardtablesview td.tright {text-align:right;}
.boardtablesview td.subject {font-weight:600; color:#000;}
.boardtablesview td.datetime {font-size:12px;}
.boardtablesview td.contents_view {padding:20px 0;}

.paging {margin:20px 0; text-align:center;}
.paging img {vertical-align:middle;}
.paging span {vertical-align:middle; padding:5px; min-width:19px; height:19px; line-height:19px; display:inline-block; font-size:12px; text-align:center; color:#757a88; font-weight:600;}
.paging a.active {vertical-align:middle; padding:5px; height:19px; line-height:19px; border:1px solid #757a88; font-size:12px; text-align:center; color:#fff; background:#757a88;}
.paging a:hover {color:orange;}

